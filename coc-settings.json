{
  "cSpell.allowCompoundWords": true,
  "cSpell.words": [
    "configurator",
    "struct",
    "trpc",
    "formo",
    "buildo",
    "hirematic",
    "pnpm",
    "craco",
    "kubernetes",
    "devspace",
    "loftsh",
    "vcluster",
    "alinity",
    "fluxcd",
    "aipclouddev",
    "syncer",
    "postgres",
    "blazor",
    "paketo",
    "paketobuildpacks"
  ],
  "explorer.icon.enableNerdfont": true,
  "explorer.file.showHiddenFiles": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "explorer.root.strategies": ["keep"],
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": ["terraform", "tf"],
      "initializationOptions": {},
      "settings": {}
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    }
  },
  "yaml.schemas": {}
}
